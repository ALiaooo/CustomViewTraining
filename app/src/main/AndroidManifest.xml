<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aliao.cvtraining" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".CanvasActivity"/>
        <activity android:name=".AigeActivity"/>
        <activity android:name=".activity.CheckedViewActivity"/>
        <activity android:name=".activity.TouchEventActivity"/>
        <activity android:name=".activity.ScrollViewPagerActivity"/>
        <activity android:name=".activity.ViewDrawingProcedureActivity"/>
        <activity android:name=".activity.CustomShowActivity"/>
        <activity android:name=".activity.PullToRefreshActivity"/>
        <activity android:name=".activity.SimpleTouchEventActivity"/>
    </application>

</manifest>
